<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/thumb_background">
        <shape
            android:shape="rectangle">

            <solid
                android:color="@color/sb_thumb_button_color_default">
            </solid>


            <corners
                android:radius="10dp">
            </corners>
        </shape>

    </item>
    <item
        android:left="15dp"
        android:right="15dp"
        android:bottom="5dp"
        android:top="5dp"
        android:gravity="center"
        android:id="@+id/thumb_image"
        android:drawable="@drawable/ic_forward" />
</layer-list>
